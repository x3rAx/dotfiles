! --- Font --------------------------------------------------------------------

xterm*faceName: Fira Code Nerd Font
xterm*faceSize: 11
xterm*renderFont: true
xterm*utf8: 2


! --- Catppuccin Mocha theme for xterm ----------------------------------------

xterm*background: #1E1E2E
xterm*foreground: #CDD6F4

! Black + Dark Grey
xterm*color0:  #45475A
xterm*color8:  #585B70

! Red + Bright Red
xterm*color1:  #F38BA8
xterm*color9:  #F38BA8

! Green + Bright Green
xterm*color2:  #A6E3A1
xterm*color10: #A6E3A1

! Yellow + Bright Yellow
xterm*color3:  #F9E2AF
xterm*color11: #F9E2AF

! Blue + Bright Blue
xterm*color4:  #89B4FA
xterm*color12: #89B4FA

! Magenta + Bright Magenta
xterm*color5:  #F5C2E7
xterm*color13: #F5C2E7

! Cyan + Bright Cyan
xterm*color6:  #94E2D5
xterm*color14: #94E2D5

! Light Grey + White
xterm*color7:  #BAC2DE
xterm*color15: #A6ADC8

! Cursor
xterm*cursorColor: #F5E0DC


! --- END ---------------------------------------------------------------------
